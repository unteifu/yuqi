{
  "name": "@yuqijs/client",
  "version": "0.0.0",
  "description": "YuqiJS - Handle your REST API calls with ease",
  "type": "module",
  "homepage": "https://yuqijs.com",
  "keywords": [
    "client",
    "fetch",
    "api",
    "zod",
    "yuqijs",
    "rest"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unteifu/yuqi.git",
    "directory": "packages/client"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsup src/index.ts --format esm --dts",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "test": "vitest run",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "devDependencies": {
    "@yuqijs/contract": "workspace:*",
    "@yuqijs/eslint-config": "workspace:*",
    "@yuqijs/prettier-config": "workspace:*",
    "@yuqijs/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tsup": "^8.3.5",
    "typescript": "catalog:",
    "vitest": "^2.1.8"
  },
  "prettier": "@yuqijs/prettier-config",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "zod": "^3.24.1"
  }
}
